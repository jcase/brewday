<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <!-- <bean/> definitions here -->

    <bean id="dataDataInputStreamSource" class="com.novust.shared.DaoDataInpuStreamSource">
        <property name="dataPathMap">
            <map>
                <entry key="com.novust.shared.data.HopData" value="/data/hopData.json" />
                <entry key="com.novust.shared.data.YeastData" value="/data/yeastData.json" />
            </map>
        </property>
    </bean>
</beans>